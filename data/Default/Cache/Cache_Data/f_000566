		<script type="text/javascript">
		
		function comprobarDatos(){
			
			$("#divDesCitado").removeClass("error");
			$("#txtDesCitadoError").html("");
			$("#txtDesCitadoErrorServer").hide();
			
			if(/\d/.test($("#txtDesCitado").val())){
				$("#divDesCitado").addClass("error");
				$("#txtDesCitadoError").html($("#alert_indicar_des_citado").val());
			}
			
			var format = /[!¿¡@#$%^&*()_+\=\[\]{};\':\"\\|,.<>\/?]+/;
			if(format.test($("#txtDesCitado").val())){
				$("#divDesCitado").addClass("error");
				$("#txtDesCitadoError").html($("#alert_indicar_des_citado").val());
			}
		}
		</script>
		
