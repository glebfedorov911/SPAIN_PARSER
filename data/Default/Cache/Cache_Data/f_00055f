        function showChat(e) {
            e.preventDefault();
            var div = document.getElementById("frame");
            console.log(div.style.display)
            if (div.style.display === "block") {
                console.log("CIERRO");
                div.style.display = "none";
            } else {
                console.log("ABRO");
                div.style.display = "block";
            }
